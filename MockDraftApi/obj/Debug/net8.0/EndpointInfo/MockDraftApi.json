          },
          "playerRank": {
            "type": "integer",
            "format": "int32"
          },
          "playerName": {
            "type": "string",
            "nullable": true
          },
          "position": {
            "type": "string",
            "nullable": true
          },
          "height": {
            "type": "string",
            "nullable": true
          },
          "weight": {
            "type": "integer",
            "format": "int32"
          },
          "college": {
            "type": "string",
            "nullable": true
          },
          "isStar": {
            "type": "boolean"
          },
          "isBust": {
            "type": "boolean"
          },
          "playerClass": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "Scoreboard": {
        "type": "object",
        "properties": {
          "ranking": {
            "type": "integer",
            "format": "int32"
          },
          "userName": {
            "type": "string",
            "nullable": true
          },
          "score": {
            "type": "integer",
            "format": "int64"
          },
          "correctFirstRoundPicks": {
            "type": "integer",
