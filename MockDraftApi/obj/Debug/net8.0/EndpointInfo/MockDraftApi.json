{
  "openapi": "3.0.1",
  "info": {
    "title": "MockDraftApi",
    "version": "1.0"
  },
  "paths": {
    "/api/getplayers": {
      "get": {
        "tags": [
          "MockDraft"
        ],
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Player"
                  }
                }
              },
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Player"
                  }
                }
              },
              "text/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Player"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/api/getteams": {
      "get": {
        "tags": [
          "MockDraft"
        ],
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Team"
                  }
                }
              },
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Team"
                  }
                }
              },
              "text/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Team"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/api/getscoreboard": {
      "get": {
        "tags": [
          "MockDraft"
        ],
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Scoreboard"
                  }
                }
              },
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Scoreboard"
                  }
                }
              },
              "text/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Scoreboard"
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Player": {
        "type": "object",
        "properties": {
          "playerId": {
            "type": "integer",
            "format": "int32"
          },
          "playerRank": {
            "type": "integer",
            "format": "int32"
          },
          "playerName": {
            "type": "string",
            "nullable": true
          },
          "position": {
            "type": "string",
            "nullable": true
          },
          "height": {
            "type": "string",
            "nullable": true
          },
          "weight": {
            "type": "integer",
            "format": "int32"
          },
          "college": {
            "type": "string",
            "nullable": true
          },
          "isStar": {
            "type": "boolean"
          },
          "isBust": {
            "type": "boolean"
          },
          "playerClass": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "Scoreboard": {
        "type": "object",
        "properties": {
          "ranking": {
            "type": "integer",
            "format": "int32"
          },
          "userName": {
            "type": "string",
            "nullable": true
          },
          "score": {
            "type": "integer",
            "format": "int64"
          },
          "correctFirstRoundPicks": {
            "type": "integer",
            "format": "int32"
          },
          "totalFirstRoundPredictions": {
            "type": "integer",
            "format": "int32"
          },
          "totalPredicionPercentage": {
            "type": "number",
            "format": "double"
          },
          "predictedTrades": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "Team": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "nullable": true
          },
          "image": {
            "type": "string",
            "nullable": true
          },
          "pickNumbersNotAdjusted": {
            "type": "string",
            "nullable": true
          },
          "pickNumbers": {
            "type": "array",
            "items": {
              "type": "integer",
              "format": "int32"
            },
            "nullable": true,
            "readOnly": true
          },
          "pickPlayersNotAdjusted": {
            "type": "string",
            "nullable": true
          },
          "pickPlayers": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "nullable": true,
            "readOnly": true
          }
        },
        "additionalProperties": false
      }
    }
  }
}